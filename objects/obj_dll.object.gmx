            <string>argstr = "";
path = obj_button_getExe.path;

if(path = "")
{
    // path empty, cancel launch
    showMessageboxOk("Cannot launch","No target executable defined.")
    setFlashingSelf(2,1000)
}

slashpos = 0

for(var i = string_length(path); i&gt;0; i--)
{
    if(string_char_at(path,i) == "\")
    {
        slashpos = i;
        break;
    }    
}

dpath = string_copy(path, 1, slashpos)



if(obj_box_nosound.checked)
{
    argstr += "-noaudio "
}

if(obj_box_output.checked)
{
    argstr += "-output "+dpath+"debug.txt "
}
if(obj_box_windowed.checked)
{
    argstr += "-inawindow "
}
if(obj_box_vanillagfx.checked)
{
    argstr += "-vanillaGFX "
}

shellExecParams("open",path,argstr)


</string>
          </argument>
        </arguments>
      </action>
    </event>
